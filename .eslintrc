{
  "plugins": ["react", "react-hooks"],

  "extends": [
    "plugin:cypress/recommended"
  ],
  "parserOptions": {
    "ecmaFeatures": {
        "jsx": true
    },
    "ecmaVersion": 2018,
    "sourceType": "module",
    "allowImportExportEverywhere": true
  },
  "parser": "babel-eslint",
  "globals": {
    "cy": true
  }
}